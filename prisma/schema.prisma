// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model CatalogoInsumos {
  idcatalogoinsumos  Int     @id @default(autoincrement()) @map("idcatalogoinsumos")
  renglon            Int?    @map("renglon")
  codigoinsumo       Int     @map("codigoinsumo")
  nombreinsumo       String  @map("nombreinsumo")
  caracteristicas    String? @map("caracteristicas")
  nombrepresentacion String? @map("nombrepresentacion")
  unidadmedida       String? @map("unidadmedida")
  codigopresentacion Int?    @map("codigopresentacion")

  @@map("catalogoinsumos")
}
